# BoomCard Partner Dashboard - Production Configuration

# Application
NODE_ENV=production
VITE_APP_NAME=BoomCard
VITE_APP_VERSION=2.0.0
VITE_APP_ENVIRONMENT=production

# API Configuration - Fly.io Backend
VITE_API_BASE_URL=https://boomcard-api.fly.dev/api
VITE_API_TIMEOUT=30000

# WebSocket Configuration
VITE_WS_URL=wss://boomcard-api.fly.dev
VITE_WS_RECONNECT_ATTEMPTS=5
VITE_WS_RECONNECT_DELAY=3000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHAT_SUPPORT=true
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE_MODE=false
VITE_ENABLE_DEBUG_MODE=false

# Cache Configuration
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=3600
